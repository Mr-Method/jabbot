#!/bin/bash
aemp gensecret
aemp seeds '*:4040' nodeid "anon/"
aemp profile jabbot-core   nodeid jabbot-core binds '*:4040'
aemp profile jabbot-memory nodeid jabbot-memory
aemp profile jabbot-irc    nodeid jabbot-irc
aemp profile jabbot-github nodeid jabbot-github
aemp profile jabbot-cpantw nodeid jabbot-cpantw
aemp profile jabbot-twitter nodeid jabbot-twitter
if [[ ! -e config/site_config.yaml ]] ; then
    cp -v config/config.yaml config/site_config.yaml
fi
